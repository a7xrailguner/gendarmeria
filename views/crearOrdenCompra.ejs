<% include header %>
<div id="page-title">
    <h2>Orden de Compra</h2>
</div>
  <div class="row-fluid">
    <div class="lr-page span4 offset4">
      <div id="register-box">
        <div class="row-fluid">
          <div id="login-form" class="span12">
            <form name="formulario" method="POST" action="/orden" >
              <div class="row-fluid">
                <br>
                Proveedor:
                <br>
                <select class="form-control" id="proveedor" name="proveedor">
                  <option value="">Seleccionar Proveedor</option>
                  
                   
                  <%  rows.forEach(function(rows){ %>
                       


                       <option ><%= rows.NombreProveedor %></option>

                        
                       <% }) %>
                       
                    </select>
                
                <br>
                Detalle:<input class="span12" id="detalle" name="detalle" type="text" value="" placeholder="Opcional"/>
              </div>
              <div class="actions">
                <button type="submit" class="btn btn-danger span12">Crear Orden de Compra</button>
              </div>
            </form>
          </div>
        </div>
      </div>
<% include footer %>
